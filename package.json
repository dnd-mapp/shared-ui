{
  "name": "@dnd-mapp/shared-ui-source",
  "description": "Official Angular component library for the D&D Mapp platform.",
  "version": "0.0.0",
  "private": true,
  "license": "UNLICENSED",
  "author": "NoNamer777",
  "homepage": "https://github.com/dnd-mapp/shared-ui",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dnd-mapp/shared-ui.git"
  },
  "bugs": {
    "url": "https://github.com/dnd-mapp/shared-ui/issues"
  },
  "packageManager": "pnpm@10.30.3",
  "workspaces": [
    "projects/*"
  ],
  "type": "module",
  "engines": {
    "node": ">=24.14.0",
    "pnpm": ">=10.30.3"
  },
  "scripts": {
    "prepublishOnly": "node tools/prepublish-only.js",
    "build": "pnpm exec ng build",
    "build-dev": "pnpm exec ng build -c development",
    "storybook:build": "pnpm exec ng run shared-ui:build-storybook",
    "storybook:start": "pnpm exec ng run shared-ui:storybook",
    "format:check": "pnpm exec prettier -l .",
    "format:write": "pnpm exec prettier -w .",
    "lint": "pnpm exec ng lint",
    "stylelint": "pnpm exec stylelint \"**/*.css\""
  },
  "dependencies": {
    "@angular/cdk": "~21.1",
    "@angular/common": "~21.1",
    "@angular/compiler": "~21.1",
    "@angular/core": "~21.1",
    "@angular/router": "~21.1",
    "rxjs": "~7.8",
    "tslib": "~2.8"
  },
  "devDependencies": {
    "@angular-eslint/builder": "~21.2",
    "@angular/build": "~21.1",
    "@angular/cli": "~21.1",
    "@angular/compiler-cli": "~21.1",
    "@eslint/js": "~9.39",
    "@storybook/addon-a11y": "~10.2",
    "@storybook/addon-docs": "~10.2",
    "@storybook/addon-onboarding": "~10.2",
    "@storybook/angular": "~10.2",
    "@tailwindcss/postcss": "~4.2",
    "@types/node": "~24.10",
    "angular-eslint": "~21.2",
    "eslint": "~9.39",
    "eslint-config-prettier": "~10.1",
    "eslint-plugin-storybook": "~10.2",
    "ng-packagr": "~21.1",
    "postcss": "~8.5",
    "prettier": "~3.8",
    "prettier-plugin-organize-imports": "~4.3",
    "storybook": "~10.2",
    "stylelint": "~17.3",
    "stylelint-config-standard": "~40.0",
    "tailwindcss": "~4.2",
    "typescript": "~5.9",
    "typescript-eslint": "~8.56"
  }
}
